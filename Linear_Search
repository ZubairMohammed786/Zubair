num = []
for i in range(0,9):
    number = int(input("Enter Number:   ",))
    num.append(number)
print("Array  :   ",num)

# Creating linear search:

found = False
Element = int(input("Enter number to be found   :  "))
i = 0
while found == False and i < len(num):
    if num[i] == Element:
        print("Element was found at index",i)
        found = True
    i = i + 1


if not found:
    print("Element is not in array")

